{
	"xsappname": "vacation",
	"tenant-mode": "dedicated",
	"scopes": [
		{
			"name": "$XSAPPNAME.staff",
			"description": "staff"
		},
		{
			"name": "$XSAPPNAME.manager",
			"description": "manager"
		}
	],

	"role-templates": [
		{
			"name": "staff",
			"description": "Staff",
			"scope-references": ["$XSAPPNAME.staff"]
		},
		{
			"name": "manager",
			"description": "Manager",
			"scope-references": ["$XSAPPNAME.manager"]
		}
	],
	"oauth2-configuration": {
		"redirect-uris": [
			"https://*.cfapps.us10-001.hana.ondemand.com/**",
			"http:localhost:8080"
		]
	}
}
